{
  "expo": {
    "name": "Hello World!",
    "slug": "hello-world",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/Logo.png",
    "splash": {
      "image": "./app/assets/Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.SalcaliMakarna.HelloWorld",
      "supportsTablet": true
    },
    "android": {
      "package": "com.SalcaliMakarna.HelloWorld",
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/Logo.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./app/assets/Logo.png"
    }
  }
}
